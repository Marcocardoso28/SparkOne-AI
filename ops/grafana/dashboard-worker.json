{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "targets": [
        {
          "expr": "increase(sparkone_worker_job_count_total[5m])",
          "legendFormat": "{{job_name}} {{status}}",
          "refId": "A"
        }
      ],
      "title": "Worker Job Executions (5m)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "id": 2,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(sparkone_worker_job_latency_seconds_bucket[5m]))",
          "legendFormat": "p95 {{job_name}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, rate(sparkone_worker_job_latency_seconds_bucket[5m]))",
          "legendFormat": "p50 {{job_name}}",
          "refId": "B"
        }
      ],
      "title": "Job Latency Quantiles",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "id": 3,
      "targets": [
        {
          "expr": "increase(sparkone_worker_delivery_total{status=\"failure\"}[15m])",
          "refId": "A"
        }
      ],
      "title": "Delivery Failures (15m)",
      "type": "stat"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "id": 4,
      "targets": [
        {
          "expr": "increase(sparkone_worker_dlq_requeues_total{status=\"success\"}[1h])",
          "refId": "A"
        }
      ],
      "title": "DLQ Reprocess Success (1h)",
      "type": "stat"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["sparkone", "worker"],
  "time": { "from": "now-6h", "to": "now" },
  "title": "SparkOne Worker",
  "version": 1
}
